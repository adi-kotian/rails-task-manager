<div class="container">
<h1>My Tasks</h1>
<% @tasks.each do |task| %>
<p>
<% if task.completed == true %>
<i class="fa-regular fa-square-check"></i>
<% else %>
<i class="fa-regular fa-square"></i>
<% end %>
<%= link_to task.title, task_path(task) %>
</p>
<% end %>
<p> <%= link_to "Create a new task", new_task_path %> <p>
</div>
